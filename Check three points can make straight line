#include<bits/stdc++.h>
using namespace std;



/*
check if three point can make straight line

(y1-y2)/(x1-x2) =  (y1 - y3)/(x1-x3)

(y1 - y2) * (x1 - x3) == (y1 - y3) * (x1 - x2)







*/
int main()
{
    std::ios_base::sync_with_stdio(0);
	cin.tie(NULL);

   int x1,x2,x3,y1,y2,y3;

   cin>>x1>>y1>>x2>>y2>>x3>>y3;
   double slope_1, slope_2;

       slope_1 = (y1-y2)*(x1-x3);
   slope_2 = (y1-y3)*(x1-x2);

if(slope_1 == slope_2)
    cout<<"YES";
else
    cout<<"NO";




}
