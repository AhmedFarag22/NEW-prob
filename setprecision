#include<iostream>
#include<iomanip>
using namespace std;
double sum(int size) {
	double sum=0;
	for (int i = 0;i < size;i++) { // 0 1 2
		double value;
		cin >> value;
		sum += value;
	}
	return sum;
}
int main()
{
	int size;
	cin >> size;
	cout << fixed << setprecision(8) << sum(size) / size << endl;
}
