#include<bits/stdc++.h>
using namespace std;



/*
Lines with the same slope are parallel
and if the slope of one line is the negative reciprocal
 of the second line, then they are perpendicular


check if the two lines are parallel

(y1-y2)/(x1-x2) =  (y1 - y3)/(x1-x3)

(y1 - y2) * (x1 - x3) == (y1 - y3) * (x1 - x2)







*/
int main()
{
    std::ios_base::sync_with_stdio(0);
	cin.tie(NULL);

   int x1,x2,x3,y1,y2,y3,x4,y4;

   cin>>x1>>y1>>x2>>y2>>x3>>y3>>x4>>y4;
   double slope_1, slope_2;

    slope_1 = (y2-y1)*(x4-x3);
   slope_2 = (y4-y3)*(x2-x1);

if(slope_1 == slope_2)
    cout<<"YES";
else
    cout<<"NO";




}
